{
  "name": "kv-manager",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev-server": "cd server && poetry run python app/main.py -- --frontend-path \"../dist\" --webserver-path \"../.web-server\"",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "generate-client": "openapi-ts --input http://localhost:11028/api/v1/openapi.json --output ./src/client --client axios",
    "install-all": "yarn install && cd server && poetry install",
    "build-server": "cd server && python -m nuitka --lto=no --standalone --onefile --output-dir=./build --output-filename=kv-manager app/main.py",
    "build-all": "yarn build && yarn build-server",
    "preview-after-build": "cp -r dist ./server/build && cp -r .web-server ./server/build && cd ./server/build && ./kv-manager"
  },
  "dependencies": {
    "@iconify/vue": "^4.1.2",
    "@radix-icons/vue": "^1.0.0",
    "@vueuse/core": "^10.9.0",
    "axios": "^1.7.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "path-browserify": "^1.0.1",
    "pinia": "^2.1.7",
    "radix-vue": "^1.8.1",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vxe-table": "^4.6.16"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.42.1",
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.12.5",
    "@vicons/ionicons5": "^0.12.0",
    "@vitejs/plugin-legacy": "^5.4.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "@vxecli/import-unplugin-vue-components": "^1.0.2",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "eslint-plugin-vue": "^9.23.0",
    "naive-ui": "^2.38.2",
    "npm-run-all2": "^6.1.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
    "terser": "^5.31.0",
    "typescript": "~5.4.0",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-vue-components": "^0.27.0",
    "vfonts": "^0.0.3",
    "vite": "^5.2.8",
    "vite-plugin-vue-devtools": "^7.0.25",
    "vue-tsc": "^2.0.11"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
